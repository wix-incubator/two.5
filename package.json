{
  "name": "two.5",
  "version": "0.0.48",
  "description": "Respond to user interactions with visual effects",
  "main": "index.js",
  "scripts": {
    "test": "test",
    "build": "rollup -c",
    "copy": "cp index.js ./docs/demo/two.5.js",
    "demo:build:single": "cd docs/demo && rollup -c single.config.js && cd -",
    "demo:build:layers": "cd docs/demo && rollup -c layers.config.js && cd -",
    "demo:build:parallax": "cd docs/demo && rollup -c parallax.config.js && cd -",
    "demo:build:background": "cd docs/demo && rollup -c background.config.js && cd -",
    "demo:build": "npm run demo:build:single && npm run demo:build:layers && npm run demo:build:parallax && npm run demo:build:background",
    "demo": "npm run build && npm run copy && npm run demo:build",
    "demo:start": "npm run demo && http-server ./docs/demo"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ydaniv/two.5.git"
  },
  "author": "Yehonatan Daniv",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/ydaniv/two.5/issues"
  },
  "homepage": "https://github.com/ydaniv/two.5#readme",
  "devDependencies": {
    "@babel/core": "^7.8.7",
    "dat.gui": "^0.7.6",
    "http-server": "^0.12.1",
    "kampos": "^0.3.5",
    "rollup": "^1.27.8",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-progress": "^1.1.1",
    "stats.js": "^0.17.0"
  },
  "dependencies": {}
}
